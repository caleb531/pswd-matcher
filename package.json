{
	"name": "pswd-matcher",
	"version": "1.1.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest run",
		"coverage": "vitest run --coverage",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@eslint/js": "^9.9.0",
		"@sveltejs/adapter-static": "^3.0.2",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@testing-library/jest-dom": "^6.4.6",
		"@testing-library/svelte": "^5.2.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/jest": "^29.5.12",
		"@vitest/coverage-v8": "^2.0.1",
		"eslint": "^9.9.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-jest": "^28.8.0",
		"eslint-plugin-svelte": "^2.43.0",
		"globals": "^15.9.0",
		"jsdom": "^24.1.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.5",
		"sass": "^1.77.6",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.4",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",
		"typescript-eslint": "^8.1.0",
		"vite": "^5.3.3",
		"vitest": "^2.0.1"
	},
	"type": "module",
	"engines": {
		"node": ">=18.0.0"
	},
	"author": "Caleb Evans",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/caleb531/pswd-matcher/issues"
	},
	"homepage": "https://github.com/caleb531/pswd-matcher#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/caleb531/pswd-matcher.git"
	}
}
