{
	"name": "pswd-matcher",
	"version": "1.1.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest run",
		"coverage": "vitest run --coverage",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.6",
		"@testing-library/user-event": "^14.6.1",
		"@types/jest": "^29.5.14",
		"@vitest/coverage-v8": "^3.0.3",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-jest": "^28.11.0",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"jsdom": "^26.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"sass": "^1.83.4",
		"svelte": "^5.19.2",
		"svelte-check": "^4.1.4",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.21.0",
		"vite": "^6.0.11",
		"vitest": "^3.0.3"
	},
	"type": "module",
	"engines": {
		"node": ">=18.0.0"
	},
	"author": "Caleb Evans",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/caleb531/pswd-matcher/issues"
	},
	"homepage": "https://github.com/caleb531/pswd-matcher#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/caleb531/pswd-matcher.git"
	},
	"pnpm": {
		"overrides": {
			"cookie@<0.7.0": ">=0.7.0"
		}
	}
}
