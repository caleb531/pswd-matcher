{
	"name": "pswd-matcher",
	"version": "1.1.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest run",
		"coverage": "vitest run --coverage",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@eslint/js": "^9.12.0",
		"@sveltejs/adapter-static": "^3.0.5",
		"@sveltejs/kit": "^2.7.0",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@testing-library/jest-dom": "^6.5.0",
		"@testing-library/svelte": "^5.2.3",
		"@testing-library/user-event": "^14.5.2",
		"@types/jest": "^29.5.13",
		"@vitest/coverage-v8": "^2.1.2",
		"eslint": "^9.12.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-jest": "^28.8.3",
		"eslint-plugin-svelte": "^2.44.1",
		"globals": "^15.11.0",
		"jsdom": "^25.0.1",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"sass": "^1.79.5",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.5",
		"tslib": "^2.7.0",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.8.1",
		"vite": "^5.4.8",
		"vitest": "^2.1.2"
	},
	"type": "module",
	"engines": {
		"node": ">=18.0.0"
	},
	"author": "Caleb Evans",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/caleb531/pswd-matcher/issues"
	},
	"homepage": "https://github.com/caleb531/pswd-matcher#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/caleb531/pswd-matcher.git"
	},
	"pnpm": {
		"overrides": {
			"cookie@<0.7.0": ">=0.7.0"
		}
	}
}
