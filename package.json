{
	"name": "pswd-matcher",
	"version": "1.1.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest run",
		"coverage": "vitest run --coverage",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@eslint/js": "^9.22.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.19.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.7",
		"@testing-library/user-event": "^14.6.1",
		"@types/jest": "^29.5.14",
		"@vitest/coverage-v8": "^3.0.8",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-jest": "^28.11.0",
		"eslint-plugin-svelte": "^3.1.0",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"sass": "^1.85.1",
		"svelte": "^5.23.0",
		"svelte-check": "^4.1.5",
		"tslib": "^2.8.1",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.1",
		"vite": "^6.2.2",
		"vitest": "^3.0.8"
	},
	"type": "module",
	"engines": {
		"node": ">=18.0.0"
	},
	"author": "Caleb Evans",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/caleb531/pswd-matcher/issues"
	},
	"homepage": "https://github.com/caleb531/pswd-matcher#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/caleb531/pswd-matcher.git"
	},
	"pnpm": {
		"overrides": {
			"cookie@<0.7.0": ">=0.7.0"
		},
		"onlyBuiltDependencies": [
			"@parcel/watcher",
			"esbuild"
		]
	}
}
